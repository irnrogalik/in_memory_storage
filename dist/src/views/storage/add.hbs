{{> header}}

<form method="POST">
    <label for="key">Key: <input type="text" title="key" name="key"></label>
    <label for="value">Value: <input type="text" title="value" name="value"></label>
    <label for="ttl">TTL: <input type="text" title="ttl" name="ttl"></label>
    <button type="submit">Add</button>
</form>

{{#if key}}
    <p>The item with <b>{{key}} = {{value}} </b> was successfully added.</p>
{{/if}}

{{> footer}}